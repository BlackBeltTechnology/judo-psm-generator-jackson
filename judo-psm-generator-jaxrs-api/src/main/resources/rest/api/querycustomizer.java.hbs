package {{ queryCustomizerPackageName self }};

/**
 * Query customizer for searching {@link {{ classFqName self }} } instances.
 */
public interface {{ queryCustomizerName self }} {
    {{# each self.relations as | relation | }}
    @io.swagger.v3.oas.annotations.media.Schema(name = "{{ relation.name }}")
    {{{ relationTargetTypeDefinition relation false }}} get{{ relation.name.firstToUpperCase }}();
